{
  "root": ".",
  "clean_urls": false,
  "https_only": true,
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/**.html": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/**.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "rewrites": [
    { "source": "/**", "destination": "/index.html" }
  ]
}
